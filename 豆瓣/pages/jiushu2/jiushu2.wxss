 .dp{
  margin-top: 80rpx;
  background: #fff;
  border-radius: 10rpx;
  padding: 30rpx 20rpx;
}
.dp_t{
  display: flex;
  justify-content: space-between;
}
.dp_t1{
  font-weight: 700;
  font-size: 34rpx;
}
.dp_t2{
  font-size: 30rpx;
  color: #818181;
}
.dp_t2 van-icon{
  font-size: 24rpx;
}
.dp_b{
  margin-top: 30rpx;
  border-bottom: 1px solid #f2f2f2;
  padding-bottom: 30rpx;
}
.dp_bt{
  display: flex;
}
.dp_bt image{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}
.dp_btr{
  padding-left: 10rpx;
}
.dp_btrz{
  font-weight: 700;
}
.dp_btr .van-rate{
  display: inline-block;
}
.dp_btr text{
  font-size: 26rpx;
  color: #c0c0c0;
  margin-left: 10rpx;
}
.dp_bc{
  margin-top: 20rpx;
  font-size: 32rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 6;
  overflow: hidden;
}
.dp_bb{
  color: #828282;
  margin-top: 20rpx;
}
.dp_bb image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  display: inline-block;
}
.dp_bb text{
  font-size: 28rpx;
  vertical-align: middle; 
}
.dp_b1{
  display: flex;
